<h1> Edit Students</h1>

<% if flash[:errors] %>
<% flash[:errors].each do |e| %>
    <p><%= e %><p> <br>
    <% end %>
   <% end %>

<%= form_for @student do |f| %>
    <%= f.label :name %>:
    <%= f.text_field :name %><br>
    <%= f.label :mod %>:
    <%= f.number_field :mod %><br>
    <%= f.submit %>
<% end %>